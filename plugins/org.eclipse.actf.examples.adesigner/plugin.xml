<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         id="org.eclipse.actf.examples.adesigner.ADesignerApplication"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.actf.examples.adesigner.ADesignerApplication">
         </run>
      </application>
   </extension>
  <extension
        point="org.eclipse.ui.preferencePages">
     <page
           category="org.eclipse.actf.ui.preferences.RootPreferencePage"
           class="org.eclipse.actf.examples.adesigner.ui.preferences.GeneralPreferencePage"
           id="org.eclipse.actf.examples.adesigner.ui.preferences.GeneralPreferencePage"
           name="%adesigner.preferences.general.title"/>
  </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.eclipse.actf.examples.adesigner.ui.preferences.PreferenceInitializer"/>
   </extension>
      <extension
            point="org.eclipse.ui.actionSets">
         <actionSet
               id="org.eclipse.actf.examples.adesigner.ui.actionSet"
               label="aDesignerClientActionSet"
               visible="true">
            <action
                  class="org.eclipse.actf.examples.adesigner.ui.actions.OpenAction"
                  id="org.eclipse.actf.examples.adesigner.ui.actions.OpenAction"
                  definitionId="org.eclipse.actf.examples.adesigner.actions.OpenAction"
                  label="%adesigner.action.open"
                  menubarPath="file/fileGroup"
                  style="push"/>
            <action
                  class="org.eclipse.actf.examples.adesigner.ui.actions.SwitchModeAction"
                  id="org.eclipse.actf.examples.adesigner.ui.actions.SwitchModeAction"
                  label="%adesigner.action.switchmode"
                  menubarPath="file/modeGroup"
                  style="push"/>
         </actionSet>
         <actionSet
               id="org.eclipse.actf.examples.adesigner.actionSet1"
               label="org.eclipse.actf.examples.adesigner.vizActionSet">
			<action
         	class="org.eclipse.actf.examples.adesigner.ui.actions.SimulationAction"
         	id="org.eclipse.actf.examples.adesigner.actions.vizLowvision"
         	definitionId="org.eclipse.actf.examples.adesigner.actions.vizLowvision"
         	label="%adesigner.action.lowvision"
         	menubarPath="visualization/"
         	style="push"/>
			<action
         	class="org.eclipse.actf.examples.adesigner.ui.actions.VisualizeAction"
         	id="org.eclipse.actf.examples.adesigner.actions.vizBlind"
         	definitionId="org.eclipse.actf.examples.adesigner.actions.vizBlind"
         	label="%adesigner.action.visualize"
         	menubarPath="visualization/"
         	style="push"/>
         </actionSet>
         <actionSet
               id="org.eclipse.actf.examples.adesigner.actionSet2"
               label="org.eclipse.actf.examples.adesigner.presentationActionSet">
            <action
         	class="org.eclipse.actf.examples.adesigner.ui.actions.PresenAuditoriumAction"
         	id="org.eclipse.actf.examples.adesigner.actions.presenAuditorium"
         	definitionId="org.eclipse.actf.examples.adesigner.actions.presenAuditorium"
         	label="%adesigner.action.presen.auditorium"
         	menubarPath="visualization/"
         	style="push"/>
            <action
         	class="org.eclipse.actf.examples.adesigner.ui.actions.PresenLargeAction"
         	id="org.eclipse.actf.examples.adesigner.actions.presenLarge"
         	definitionId="org.eclipse.actf.examples.adesigner.actions.presenLarge"
         	label="%adesigner.action.presen.large"
         	menubarPath="visualization/"
         	style="push"/>
            <action
         	class="org.eclipse.actf.examples.adesigner.ui.actions.PresenSmallAction"
         	id="org.eclipse.actf.examples.adesigner.actions.presenSmall"
         	definitionId="org.eclipse.actf.examples.adesigner.actions.presenSmall"
         	label="%adesigner.action.presen.small"
         	menubarPath="visualization/"
         	style="push"/>               
         </actionSet>
      </extension>
         
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="org.eclipse.actf.visualization.ui.perspectives.HTMLPerspective">
         <actionSet id="org.eclipse.actf.examples.adesigner.actionSet1"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="org.eclipse.actf.visualization.ui.perspectives.ODFPerspective">
         <actionSet id="org.eclipse.actf.examples.adesigner.actionSet1"/>
         <actionSet id="org.eclipse.actf.examples.adesigner.actionSet2"/>
      </perspectiveExtension>
   </extension>
   
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.actf.examples.adesigner.actions.OpenAction"
            name="%adesigner.action.open"/>
      <command
            id="org.eclipse.actf.examples.adesigner.actions.vizBlind"
            name="%adesigner.action.visualize"/>
      <command
            id="org.eclipse.actf.examples.adesigner.actions.vizLowvision"
            name="%adesigner.action.lowvision"/>
      <command
            id="org.eclipse.actf.examples.adesigner.actions.presenAuditorium"
            name="%adesigner.action.presen.auditorium"/>
      <command
            id="org.eclipse.actf.examples.adesigner.actions.presenLarge"
            name="%adesigner.action.presen.large"/>
      <command
            id="org.eclipse.actf.examples.adesigner.actions.presenSmall"
            name="%adesigner.action.presen.small"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.actf.examples.adesigner.actions.OpenAction"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+O"/>
      <key
            commandId="org.eclipse.actf.examples.adesigner.actions.vizBlind"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+B"/>
      <key
            commandId="org.eclipse.actf.examples.adesigner.actions.vizLowvision"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+O"/>
      <key
            commandId="org.eclipse.actf.examples.adesigner.actions.presenAuditorium"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+A"/>
      <key
            commandId="org.eclipse.actf.examples.adesigner.actions.presenLarge"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+G"/>
      <key
            commandId="org.eclipse.actf.examples.adesigner.actions.presenSmall"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+M"/>
   </extension>
   <extension
         id="aDesigner"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.actf.examples.adesigner.org.eclipse.actf.examples.adesigner.ADesignerApplication"
            name="aDesigner">
         <property
               name="windowImages"
               value="icons/excla_32.png">
         </property>
         <property
               name="aboutText"
               value="%About.Text">
         </property>
         <property
               name="aboutImage"
               value="about.gif">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="0,318,500,12">
         </property>
         <property
               name="startupMessageRect"
               value="10,203,500,20">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="appName"
               value="aDesigner">
         </property>
      </product>
   </extension>

</plugin>
